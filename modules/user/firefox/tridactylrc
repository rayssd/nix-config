" General Settings
set editorcmd /Applications/Alacritty.app/Contents/MacOS/alacritty -e /bin/zsh -lc 'nvim %f "+call cursor(%l, %c)"'
set searchurls.docs https://docs.mongodb.com/search/?q=%s
set searchurls.kf https://search.corp.mongodb.com/#q=%s&sort=relevancy
set smoothscroll true
set configversion 2.0
set update.lastchecktime 1707094156841

" Binds
unbind d
bind x tabclose
bind b fillcmdline taball

" For syntax highlighting see https://github.com/tridactyl/vim-tridactyl
" vim: set filetype=tridactyl