{
  "title": "aerospace shortcuts",
  "rules": [
    {
      "description": "[aerospace] Fn-number to switch workspace",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "1", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace workspace 1" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "2", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace workspace 2" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "3", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace workspace 3" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "4", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace workspace 4" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "5", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace workspace 5" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "6", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace workspace 6" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "7", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace workspace 7" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "8", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace workspace 8" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "9", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace workspace 9" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "0", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace workspace 0" } ]
        }
      ]
    },
    
    {
      "description": "[aerospace] Shift+Fn-number to move to workspace",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "1", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move-node-to-workspace 1" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "2", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move-node-to-workspace 2" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "3", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move-node-to-workspace 3" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "4", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move-node-to-workspace 4" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "5", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move-node-to-workspace 5" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "6", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move-node-to-workspace 6" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "7", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move-node-to-workspace 7" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "8", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move-node-to-workspace 8" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "9", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move-node-to-workspace 9" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "0", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move-node-to-workspace 0" } ]
        }
      ]
    },

    {
      "description": "[aerospace] Fn-yuio to move focus",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "y", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace focus left" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "u", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace focus down" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "i", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace focus up" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "o", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace focus right" } ]
        }
      ]
    },
    {
      "description": "[aerospace] Shift+Fn-hjkl to move window",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "h", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move left" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "j", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move down" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "k", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move up" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "l", "modifiers": { "mandatory": ["fn", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace move right" } ]
        }
      ]
    },
    {
      "description": "[aerospace] Fn-misc keys",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "hyphen", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace resize smart -50" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "equal_sign", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace resize smart +50" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "slash", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace layout tiles horizontal vertical" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "period", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace layout tiles horizontal vertical" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "comma", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace layout accordion horizontal vertical" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "tab", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace focus-back-and-forth || /opt/homebrew/bin/aerospacekworkspace-back-and-forth" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "f", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace layout floating tiling" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "b", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace balance-sizes" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "m", "modifiers": { "mandatory": ["fn"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace macos-native-fullscreen" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "semicolon", "modifiers": { "mandatory": ["fn", "control", "shift"] } },
          "to": [ { "shell_command": "/opt/homebrew/bin/aerospace mode service" } ]
        },
        {
          "type": "basic",
          "from": { "key_code": "r", "modifiers": { "mandatory": ["fn", "control", "shift"] } },
          "to": [ { "shell_command": "launchctl kickstart -k gui/$UID/bobko.aerospace" } ]
        }
      ]
    }
  ]
}
